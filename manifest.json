{
  "manifest_version": 2,
  "name": "CPH Submit",
  "version": "1.0",
  "description": "Codeforces Submit add-on for Competitive Programming Helper",
  "content_scripts": [
    {
      "matches": ["*://https://codeforces.com/problemset/submit/*"],
      "js": ["dist/submitPageScript.js"]
    }
  ],
  "background": {
    "scripts": ["dist/backgroundScript.js"]
  },
  "permissions": ["<all_urls>", "tabs"]
}
