{
  "name": "CPH Submit Chromium",
  "manifest_version": 3,
  "homepage_url": "https://github.com/agrawal-d/cph-submit",
  "version": "1.4.1",
  "description": "Codeforces Submit add-on for Competitive Programming Helper for Chromium browsers.",
  "background": {
    "service_worker": "dist/backgroundScript.js"
  },
  "host_permissions": [
    "http://localhost/*",
    "https://localhost/*",
    "http://127.0.0.1/*",
    "https://127.0.0.1/*",
    "http://codeforces.com/*",
    "https://codeforces.com/*"
  ],
  "permissions": [
    "webNavigation",
    "activeTab"
  ],
  "icons": {
    "48": "icon-48.png"
  },
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["dist/injectedScript.js"],
    "run_at": "document_end"
  }
  ]

}

